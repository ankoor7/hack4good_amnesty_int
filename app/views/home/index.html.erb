<h1>Home#index</h1>
<p>Find me in app/views/home/index.html.erb</p>
<h2>Upcoming Events</h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
      <th></th>
    </tr>
  <thead>
  <tbody id="events">
    <%= render @events %>
  <tbody>
</table>


<div id="events_paginator">
  <%= paginate @events, :remote => true %>
</div>

<h2>Groups</h2>
<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Description</th>
      <th></th>
    </tr>
  <thead>
  <tbody id="groups">
    <%= render @groups %>
  <tbody>
</table>


<div id="groups_paginator">
  <%= paginate @groups, :remote => true %>
</div>


