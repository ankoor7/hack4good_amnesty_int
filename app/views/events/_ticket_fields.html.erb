    <div class="form-group">
      <%= f.fields_for :ticket_blocks do |ticket_block_form| %>
        <%= ticket_block_form.label "Name", class: "sr-only"  %>
        <%=ticket_block_form.text_field :name, placeholder: "Name", class: "form-control"  %>
        <%= ticket_block_form.label "Quantity", class: "sr-only"  %>
        <%= ticket_block_form.select :quantity, ((1..30).map {|i| [i,i] }), {}, {class: "form-control"} %>
      <% end %>
      <%= f.link_to_add "Add a type of ticket", :ticket_blocks %>
    </div>
