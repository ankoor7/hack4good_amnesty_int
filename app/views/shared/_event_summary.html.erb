<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Date</th>
    </tr>
  <thead>
  <tbody>
    <% events.each do |event| %>
      <tr>
        <td><%= link_to event.name, event %></td>
        <td><%= event.date.strftime("%m/%d/%Y at %I:%M%P")  %></td>
      </tr>
    <% end %>
  <tbody>
</table>
